#include<stdio.h>
int main()
{
    int t;
    double number;
    scanf("%d",&t);
    while(t>=0)
    {
         scanf("%lf",&number);
        if(number<=0)
        {
            if(number==0)
            {
                printf("The number is :0");
            }
            else
            {
                printf("The number is negative");
            }
        }
            else
            {
                printf("The number is positive");
            }
       t--;     
    }
    return 0;
}
